<p id="notice"><%= notice %></p>

<h1>Membership Orders</h1>

<table>
  <thead>
    <tr>
      <th>Membership</th>
      <th>User</th>
      <th>Paid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membership_orders.each do |membership_order| %>
      <tr>
        <td><%= membership_order.membership %></td>
        <td><%= membership_order.user %></td>
        <td><%= membership_order.paid %></td>
        <td><%= link_to 'Show', membership_order %></td>
        <td><%= link_to 'Edit', edit_membership_order_path(membership_order) %></td>
        <td><%= link_to 'Destroy', membership_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membership Order', new_membership_order_path %>
