<% if @books.any? %>

<h1>Novedades</h1>

    <table>
      <thead>
        <tr>
          <th></th>
          <th>TÃ­tulo</th>
          <th>Autor</th>
          <th>Apellido</th>
          <th> Editorial </th>
          <th>Precio</th>
          <th>ISBN</th>
          <th>GÃ©neros</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @books.each do |book| %>
          <tr>
            <td><%= link_to image_tag("#{book.cover.thumb.url}", :alt => book.title), book_path(book.id) %></td>
            <td><%= book.title %></td>
            <td><%= book.author_first_name %></td>
            <td><%= book.author_last_name %></td>
            <td> <%= link_to "#{book.publisher.name}", publisher_path(book.publisher.id) %></td>
            <td><%= book.price %></td>
            <td><%= book.isbn %></td>
            <td><%= book.genres.pluck(:name).join(", ") %></td>
            <td><%= link_to 'Detalle', book %></td>
            <% if signed_in? && current_user.role == 'Lector' %>
            <td><%= link_to 'Comprar', book_orders_path(book), method: :post %> </td>
            <% else %>
            <td> Ingresa con tu cuenta de lector para comprar </td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
<% else %>
<p> No hay libros con esos datos </p>
<% end %>
